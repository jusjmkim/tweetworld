      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

  </head>
  <body>
    <header>
    <a href="/map" class="btn btn-default btn-sm">Map</a>
    <% if (login) { %>
      <a href="/logout" class="btn btn-default btn-sm">Log Out</a>
    <% } else { %>
      <a href="/login" class="btn btn-default btn-sm">Log In</a>
      <a href="/signup" class="btn btn-default btn-sm">Sign Up</a>
    <% } %>
    </header>

    <form id="country_form" method="post" action="http://localhost:8080">
      <select id="country">
        <script src="javascripts/countries2.js"></script>
      </select>
      <input type="submit" value="Submit" id="submit">
    </form>
    
    <div class="tweet_country_holder"></div>

     <!-- Latest compiled and minified bootstrap JavaScript -->
      <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
  </body>
</html>